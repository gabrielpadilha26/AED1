#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAXN 10005
#define MAXM 30005

int head[MAXN];
int to[MAXM];
int next_edge[MAXM];
int edge_count;
int visited[MAXN]; 

void init(int n) {
    edge_count = 0;
    for (int i = 0; i <= n; i++) {
        head[i] = -1;
        visited[i] = 0; 
    }
}

void add_edge(int u, int v) {
    to[edge_count] = v;
    next_edge[edge_count] = head[u];
    head[u] = edge_count++;
}

int dfs(int u) {
    visited[u] = 1; 

    for (int i = head[u]; i != -1; i = next_edge[i]) {
        int v = to[i];
        if (visited[v] == 1) {
            return 1; 
        }
        if (visited[v] == 0) {
            if (dfs(v)) return 1;
        }
    }

    visited[u] = 2; 
    return 0;
}

int main() {
    int T;
    if (scanf("%d", &T) != 1) return 0;

    while (T--) {
        int N, M;
        scanf("%d %d", &N, &M);

        init(N);

        for (int i = 0; i < M; i++) {
            int u, v;
            scanf("%d %d", &u, &v);
            add_edge(u, v);
        }

        int cycle_found = 0;
        for (int i = 1; i <= N; i++) {
            if (visited[i] == 0) {
                if (dfs(i)) {
                    cycle_found = 1;
                    break;
                }
            }
        }

        if (cycle_found)
            printf("SIM\n");
        else
            printf("NAO\n");
    }

    return 0;
}
