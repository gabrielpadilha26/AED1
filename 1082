#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int adj[26][26];
int visited[26];
int component[26];
int comp_size;
int V, E;

int compare(const void *a, const void *b) {
    return (*(int*)a - *(int*)b);
}

void dfs(int u) {
    visited[u] = 1;
    component[comp_size++] = u;
    for (int v = 0; v < V; v++) {
        if (adj[u][v] && !visited[v]) {
            dfs(v);
        }
    }
}

int main() {
    int N, case_num = 1;
    char u, v;

    scanf("%d", &N);

    while (N--) {
        scanf("%d %d", &V, &E);

        memset(adj, 0, sizeof(adj));
        memset(visited, 0, sizeof(visited));

        for (int i = 0; i < E; i++) {
            scanf(" %c %c", &u, &v);
            adj[u - 'a'][v - 'a'] = 1;
            adj[v - 'a'][u - 'a'] = 1;
        }

        printf("Case #%d:\n", case_num++);

        int connected_components = 0;

        for (int i = 0; i < V; i++) {
            if (!visited[i]) {
                connected_components++;
                comp_size = 0;
                dfs(i);
                
                qsort(component, comp_size, sizeof(int), compare);

                for (int j = 0; j < comp_size; j++) {
                    printf("%c,", component[j] + 'a');
                }
                printf("\n");
            }
        }

        printf("%d connected components\n\n", connected_components);
    }

    return 0;
}
