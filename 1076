#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        int start;
        scanf("%d", &start);
        
        int V, A;
        scanf("%d %d", &V, &A);
        
        int seen[100][100] = {0};
        int u, v;
        int unique = 0;
        
        for (int i = 0; i < A; ++i) {
            scanf("%d %d", &u, &v);
            int a = u < v ? u : v;
            int b = u < v ? v : u;
            if (!seen[a][b]) {
                seen[a][b] = 1;
                unique++;
            }
        }
        
        printf("%d\n", 2 * unique);
    }
    return 0;
}
