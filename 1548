#include <stdio.h>
#include <stdlib.h>

int cmp(const void *a, const void *b) {
    return *(int*)b - *(int*)a;
}

int main() {
    int N, M;
    scanf("%d", &N);

    while (N--) {
        scanf("%d", &M);

        int orig[1000], sorted[1000];
        for (int i = 0; i < M; i++) {
            scanf("%d", &orig[i]);
            sorted[i] = orig[i];
        }

        qsort(sorted, M, sizeof(int), cmp);

        int count = 0;
        for (int i = 0; i < M; i++)
            if (sorted[i] == orig[i])
                count++;

        printf("%d\n", count);
    }

    return 0;
}
