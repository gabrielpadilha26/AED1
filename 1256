#include <stdio.h>
#include <stdlib.h>

typedef struct Node {
    int value;
    struct Node *next;
} Node;

void insert(Node **table, int index, int val) {
    Node *newNode = (Node *)malloc(sizeof(Node));
    newNode->value = val;
    newNode->next = NULL;

    if (table[index] == NULL) {
        table[index] = newNode;
    } else {
        Node *temp = table[index];
        while (temp->next != NULL) {
            temp = temp->next;
        }
        temp->next = newNode;
    }
}

int main() {
    int N;
    scanf("%d", &N);
    
    int first = 1;

    while (N--) {
        int M, C;
        scanf("%d %d", &M, &C);

        Node **table = (Node **)malloc(M * sizeof(Node *));
        for (int i = 0; i < M; i++) {
            table[i] = NULL;
        }

        for (int i = 0; i < C; i++) {
            int val;
            scanf("%d", &val);
            insert(table, val % M, val);
        }

        if (!first) {
            printf("\n");
        }
        first = 0;

        for (int i = 0; i < M; i++) {
            printf("%d -> ", i);
            Node *temp = table[i];
            while (temp != NULL) {
                printf("%d -> ", temp->value);
                Node *toFree = temp;
                temp = temp->next;
                free(toFree);
            }
            printf("\\\n");
        }

        free(table);
    }

    return 0;
}
