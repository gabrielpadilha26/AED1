#include <stdio.h>

int grid[1005][1005];

int main() {
    int N, M;
    scanf("%d %d", &N, &M);

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < M; j++) {
            scanf("%d", &grid[i][j]);
        }
    }

    for (int i = 1; i < N - 1; i++) {
        for (int j = 1; j < M - 1; j++) {
            if (grid[i][j] == 42) {
                if (grid[i-1][j-1] == 7 && grid[i-1][j] == 7 && grid[i-1][j+1] == 7 &&
                    grid[i][j-1]   == 7 &&                      grid[i][j+1]   == 7 &&
                    grid[i+1][j-1] == 7 && grid[i+1][j] == 7 && grid[i+1][j+1] == 7) {
                    
                    printf("%d %d\n", i + 1, j + 1);
                    return 0;
                }
            }
        }
    }

    printf("0 0\n");

    return 0;
}
